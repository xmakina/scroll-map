{
  "cSpell.words": ["adamscybot"],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/{info,lfs,logs,refs,objects}/**",
    ".git/{index,*refs,*HEAD}",
    ".vscode",
    ".vscode-insiders",
    ".env",
    "*config*"
  ],
  "eslint.workingDirectories": ["./"],
  "vsicons.associations.files": [
    {
      "icon": "testts",
      "extensions": ["integration.ts", "e2e.ts"],
      "format": "svg"
    }
  ],
  "i18n-ally.localesPaths": ["messages", "src/i18n"],
  "i18n-ally.keystyle": "nested"
}
